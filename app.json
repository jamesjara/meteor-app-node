{
  "name": "spartug",
  "description": "The open-source Trello-like kanban",
  "repository": "https://github.com/spartug/spartug",
  "logo": "https://raw.githubusercontent.com/spartug/spartug/master/meta/icons/spartug-150.png",
  "keywords": ["productivity", "tool", "team", "kanban"],
  "website": "http://spartug.io",
  "env": {
    "BUILDPACK_URL": "https://github.com/AdmitHub/meteor-buildpack-horse.git",
    "ROOT_URL": {
      "description": "IMPORTANT! Please replace <App Name> with the value provided on the top. This will be the full URL of your spartug app.",
      "value": "https://<App Name>.herokuapp.com"
    }
  },
  "addons": [
    "mongolab",
    "logentries"
  ]
}
